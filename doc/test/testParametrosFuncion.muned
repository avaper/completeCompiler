MODULE LEVEL0;
  	
  VAR 
    A : INTEGER;
  	C : INTEGER;
  	D : INTEGER;
  	B : INTEGER;
  	X : INTEGER;
  	
  PROCEDURE FUNC (VAR X, Y, Z, W: INTEGER): INTEGER;
  		
  BEGIN
	
	 X := 1;
	 Y := 2;
	 Z := 3;
	 W := 4;
	 
	 RETURN 42;
  	  
  END FUNC;
	
BEGIN
  
  X := FUNC(A, B, C, D);
  		
  WRITESTRING("Valor de A (1): ");
  WRITEINT(A);
  WRITELN;

  WRITESTRING("Valor de B (2): ");
  WRITEINT(B);
  WRITELN;
  
  WRITESTRING("Valor de C (3): ");
  WRITEINT(C);
  WRITELN;

  WRITESTRING("Valor de D (4): ");
  WRITEINT(D);
  WRITELN;
    
END LEVEL0;